<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.locator_app.playground.bubblesample" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/locatorlogo"
        android:logo="@drawable/locatorlogo"
        android:label="Locator-BubbleSample"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.locator_app.playground.bubblesample.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
